
public class DoTheMathTheMonsterMath {
	public static void main(String[] args)
	{
		//DO SOMETHing
		int v = 120;
		int t = divide(v);
		System.out.println(t + "!");
	}
	public static int divide(double num)
	{
		int b = 1;
		boolean a = true;
		while (a) 
		{
			b++;
			num = num/b;
			System.out.println(num);
			if (!((num%(b-1))==0)) {
				return 0;
			} else if (num==1) {
				a = false;
		}
		}
		System.out.println(b);
		return b;
	}
}
