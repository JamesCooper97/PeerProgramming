import java.util.ArrayList;

public class DoggoFinder {
	
	ArrayList<BigScruff> kindabigscruff = new ArrayList<BigScruff>();
	public void method1(BigScruff yeahboi)
	{
		kindabigscruff.add(yeahboi);
	}
	public int doggoFinder(String heya)
	{
		int j = 0;
		for (BigScruff i : kindabigscruff)
			if (i.getName()==heya) {
				return j = i.getPlace();
			}
		return j;
	}
}
