
public class HelloWorld {	

	public static void main(String[] args) {
		int[] arrayofsums = new int[10];
	
		int a = 1; int b = 2; int c = 1; int temp = 70; boolean summer = true;// boolean condition = false;
		// int sum = sum(a,b,c);
		boolean isinsummer = temperature(temp, summer);
		System.out.println(isinsummer);
		
		//for (int i = 0; i<10; i++) {
			//arrayofsums[i] = sum;
			//System.out.println(sum);
		//}
		//for (int i=0; i<arrayofsums.length; i++) {
			//System.out.println(arrayofsums[i]);
		//}
		for (int i=0; i<10; i++) {
			arrayofsums[i] = i;
			System.out.println(arrayofsums[i]);
		}
		for (int i=0; i<10; i++) {
			arrayofsums[i] = i*10;
			System.out.println(arrayofsums[i]);
		}
	}
	
	
	static String printWorld() {
		String str = "hello world";
		return str;
	}
	
	
	static int sum(int a, int b, int c) {
		int sum;
		if (a==b) {
			sum = c; 
		}
		else if(a==c) {
			sum = b;
		}
		else if (b==c) {
			sum = a;
		}
		else {
			sum = a + b + c;
		}
		return sum;
	}
	
	static boolean temperature(int temp, boolean summer) {
		boolean isintemp = false;;
		if (summer = true) {
			if(temp >= 60 && temp <= 80) {
				isintemp = true;
			}
		}
		else if (summer = false) {
			if(temp >= 60 && temp <= 90) {
				isintemp = true;
			}
		}
		return isintemp;
	}
	
}


