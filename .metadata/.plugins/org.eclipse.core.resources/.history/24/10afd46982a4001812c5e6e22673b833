
public class HelloWorld {	

	public static void main(String[] args) {
		int[] arrayofsums = new int[10];
	
		int a = 1; int b = 2; boolean condition = false;
		int sum = sum(a,b, condition);
		System.out.println(printWorld());
		//for (int i = 0; i<10; i++) {
			//arrayofsums[i] = sum;
			//System.out.println(sum);
		//}
		//for (int i=0; i<arrayofsums.length; i++) {
			//System.out.println(arrayofsums[i]);
		//}
		for (int i=0; i<10; i++) {
			arrayofsums[i] = i;
			System.out.println(arrayofsums[i]);
		}
		for (int i=0; i<10; i++) {
			arrayofsums[i] = i*10;
			System.out.println(arrayofsums[i]);
		}
	}
	
	
	static String printWorld() {
		String str = "hello world";
		return str;
	}
	
	
	static int sum(int a, int b, boolean condition) {
		int sum;
		if (condition = true){
		sum = a + b;
		}
		else {
			sum = a*b;
		}
		
		return sum;
	}
	
}


