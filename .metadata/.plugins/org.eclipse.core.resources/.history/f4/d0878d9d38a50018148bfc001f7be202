import java.util.*;

public class Main {
	
public static void main(String[] args) {
	
	ArrayList<Object> listOfPeople = new ArrayList<Object>(); 
	
	Person jordan = new Person();
	
	jordan.setName("Jordan");
	jordan.setAge(22);
	jordan.setJob("Trainee");
	
	// System.out.println(jordan.toString());
	
	Person alan = new Person();
	
	alan.setName("Alan");
	alan.setAge(31);
	alan.setJob("Consultant");
	
	Person steve = new Person();
	
	steve.setName("Steve");
	steve.setAge(31);
	steve.setJob("Consultant");
	
	listOfPeople.add(jordan);
	listOfPeople.add(alan);
	listOfPeople.add(steve);
	
	for(int i = 0; i<listOfPeople.size(); i++) {
		System.out.println(listOfPeople.get(i).toString());
	}
	
	for(int i = 0; i<listOfPeople.size(); i++) {
		if (((Person) listOfPeople.get(i)).getName() == "Steve") {
		System.out.println(listOfPeople.get(i).toString());
		}
	}
}
}
